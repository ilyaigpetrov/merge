    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "mocha": "^8.2.1",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sort-json": "^1.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
